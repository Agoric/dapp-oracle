    "ava": "^3.12.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "0.0.3",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^37.9.0",
    "eslint-plugin-prettier": "^3.1.0",
    "nyc": "^15.1.0",
    "prettier": "^1.18.2",
